{
  "author": "Vincent V. Toscano",
  "license": "UNLICENSED",
  "private": true,
  "bugs": {
    "url": "https://github.com/VincentVToscano/Rollup_2021-001/issues"
  },
  "homepage": "https://github.com/VincentVToscano/Rollup_2021-001#readme",
  "name": "rollup_2021-001",
  "version": "1.0.0",
  "description": "Vetting Rollup.js to determine how adaptable it is to an array of project requirements.",
  "main": "index.html",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VincentVToscano/Rollup_2021-001.git"
  },
  "scripts": {
    "build": "rollup -c --environment INCLUDE_DEPS,BUILD:prod",
    "build-babel": "rollup -c --environment INCLUDE_DEPS,BUILD:prod,TRANSFORM",
    "build-dev": "rollup -c --environment INCLUDE_DEPS,BUILD:dev",
    "watch": "rollup -c -w --watch.exclude 'node_modules/**' --environment INCLUDE_DEPS,BUILD:dev --silent",
    "watch-with-serve": "rollup -c -w --watch.exclude 'node_modules/**' --environment INCLUDE_DEPS,BUILD:dev,SERVE --silent",
    "serve-dev": "serve .",
    "dev-serve": "npm-run-all --parallel watch-with-serve",
    "dev": "npm-run-all --parallel watch"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "lottie-web": "^5.7.13",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.56.3",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.39.0",
    "serve": "^12.0.1"
  },
  "keywords": [
    "rollup",
    "task",
    "runner",
    "bundling"
  ]
}
